<!doctype html>

<html lang="en">
<head>
    <meta charset="utf-8">
    <title>Kirill Zaytsev CV</title>
    <meta name="description" content="Kirill Zaytsev CV">
    <meta name="author" content="Kirill Zaytsev">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i" rel="stylesheet">
    <link rel="stylesheet" href="index.css">
</head>

<body>

    <div class="paper-page" size="A4">
        <%- include('partials/header', person); %>
        <div class="paper-page__content">
            <%- include('partials/overview', overview); %>
            <section class="skill-set">

                <h2 class="skill-set__header">
                    Skill set
                </h2>

                <table class="skill-set__common-head">
                    <thead>
                        <th class="skill-set__common-col skill-set__common-col--tech">Technology, Tool, Skill</th>
                        <th class="skill-set__common-col skill-set__common-col--exp">Experience (years)</th>
                        <th class="skill-set__common-col skill-set__common-col--last">Last time used</th>
                    </thead>
                </table>

                <%- include('partials/skill-table', skills.languages); %>
                <%- include('partials/skill-table', skills.frameworks); %>

            </section>
        </div>
        <%- include('partials/footer', {}); %>
    </div>

    <div class="paper-page" size="A4">
        <%- include('partials/header', person); %>
        <div class="paper-page__content">

            <section class="skill-set">
                <%- include('partials/skill-table', skills.dbms); %>
                <%- include('partials/skill-table', skills.appservers); %>
                <%- include('partials/skill-table', skills.tools); %>
            </section>

            <%- include('partials/languages', {items: languages}); %>
            <%- include('partials/education', education); %>
        </div>
        <%- include('partials/footer', {}); %>
    </div>


    <div class="paper-page" size="A4">
        <%- include('partials/header', person); %>
        <div class="paper-page__content">

            <section class="projects">

                <h2 class="projects__header" data-aux="examples of recent projects">
                    Professional experience
                </h2>

                <%- include('partials/project-table', {item: projects.cat}); %>
                <%- include('partials/project-table', {item: projects.mobile}); %>

            </section>
        </div>
        <%- include('partials/footer', {}); %>
    </div>

    <div class="paper-page" size="A4">
        <%- include('partials/header', person); %>
        <div class="paper-page__content">

            <section class="projects">

                <%- include('partials/project-table', {item: projects.scada}); %>
                <%- include('partials/project-table', {item: projects.qmf}); %>

            </section>
        </div>
        <%- include('partials/footer', {}); %>
    </div>

</body>

</html>
